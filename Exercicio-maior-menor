a1 = int(input("Primeiro numero: "))
a2 = int(input("Segundo numero: "))
a3 = int(input("Terceiro numero: "))

if a1 >= a2:
    maior = a1
else:
    maior =a2

if maior < a3:
    maior = a3

if a1 <= a2:
    menor = a1
else:
    menor =a2

if menor > a3:
    menor = a3



print("O maior numero: {0}".format(str(maior)))
print("O menor numero: {0}".format(str(menor)))
